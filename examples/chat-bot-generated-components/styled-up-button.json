    "styles": {
      "css": ".styled-btn {\n  padding: 10px 20px;\n  border: none;\n  border-radius: 5px;\n  font-size: 16px;\n  font-weight: bold;\n  cursor: pointer;\n  background-color: var(--primary-color, #3498db);\n  color: white;\n  transition: all 0.3s ease;\n}\n\n.styled-btn:hover {\n  transform: translateY(-2px);\n  box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);\n}\n\n.styled-btn.primary {\n  background-color: var(--primary-color, #3498db);\n}\n\n.styled-btn.secondary {\n  background-color: var(--secondary-color, #f39c12);\n}\n\n.styled-btn.large {\n  padding: 12px 24px;\n  font-size: 18px;\n}\n\n.styled-btn.small {\n  padding: 8px 16px;\n  font-size: 14px;\n}",
      "variables": {
        "text": "Click Me",
        "variant": "primary",
        "size": "medium",
        "disabled": false
      },
      "library": {
        "css": ".styled-btn { padding: 8px 16px; font-size: 14px; }",
        "variables": {
          "text": "Button",
          "variant": "primary",
          "size": "medium"
        }
      }
    },
    "icon": {
      "mode": "emoji",
